{
	"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Steam Account Protection",
	"version": "3.3.5",
	"manifest_version": 2,
	"description": "Help protect your Steam Account",
	"background": {
		"scripts": [ "main/background.js" ]
	},
	"externally_connectable": {
		"matches": []
	},
	"icons": {
		"16": "images/sap_16.png",
		"32": "images/sap_32.png",
		"64": "images/sap_64.png",
		"128": "images/sap_128.png",
		"512": "images/sap_512.png"
	},
	"content_scripts": [
		{
			"js": [ "thirdparty/jquery_3.4.1_slim.js", "thirdparty/Resemble.js/Resemble.js", "main/utilities.js", "main/trade_window.js", "main/profile.js", "main/user_scanners.js", "main/apis.js", "html/elements/html_elements.js", "main/main.js" ],
			"matches": [ "https://steamcommunity.com/dev/apikey", "https://steamcommunity.com/profiles/*", "https://steamcommunity.com/id/*", "https://steamcommunity.com/tradeoffer/*", "https://backpack.tf/api/IGetUsers/v3", "https://backpack.tf/api/IGetUsers/GetImpersonatedUsers", "https://marketplace.tf/api/Bots/GetBots/v2", "https://mannco.store/bots", "https://steamrep.com/api/beta4/reputation/*" ]
		}
	],
	"permissions": [ "activeTab", "storage" ],
	"web_accessible_resources": [ "images/*.png", "html/*" ],
	"browser_action": {
		"default": "images/sap_512.png"
	}
}
